{
  "name": "Trello My Cards",
  "version": "1.5",
  "manifest_version": 1,
  "description": "Lists Trello card in which i am engaged.",
  "browser_action": {
     "default_icon": "icon.png",
	"default_popup": "popup.html"
  },
  "permissions": [
    "https://trello.com/"
  ],
  "content_security_policy": "script-src 'unsafe-eval' 'unsafe-inline' 'self' https://ssl.google-analytics.com https://trello.com https://api.trello.com"
}